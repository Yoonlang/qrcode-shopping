{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": ["esnext", "dom"],
    "types": ["cypress", "node", "./support"],
    "esModuleInterop": true,
    "paths": {
      "cypress/*": ["./cypress/*"],
      "src/*": ["../src/*"]
    }
  },
  "include": ["./**/*.ts", "../cypress.config.ts"]
}
